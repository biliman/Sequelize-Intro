<!DOCTYPE html>
<html>
  <head>
    <title>add subject</title>
    <%- include layout/link_head %>
  </head>
  <body>
    <%- include layout/header %>
    
    <div class="container">
      <h3>SUBJECT ADD FORM</h3>
      <form action="/subjects/add" method="post" class="form-horizontal">
        <div class="form-group">
          <label class="control-label col-sm-2">Subject Name :</label>
          <div class="col-sm-10">
            <p class="form-control-static"><input type="text" name="subject"/></p>
          </div>
        </div>
        
        <div class="form-group">
          <label class="control-label col-sm-2"></label>
          <div class="col-sm-10">
            <input type="submit" class="btn btn-success" value="ADD Subject"/> 
          </div>
        </div>
    </div>
    
  </body>
</html>