<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>teachers</title>
    <link rel="stylesheet" href="/style.css">
  </head>
  <body>
    <!-- <%= JSON.stringify(teachers) %> -->
    <table id="customers">
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% teachers.forEach(teacher => { %>
      <tr>
        <td><%= teacher.id %></td>    
        <td><%=teacher.first_name %></td>
        <td><%=teacher.last_name %></td>
        <td><%=teacher.email %></td>    
      </tr>
      <% }) %>
    </table>
  </body>
</html>